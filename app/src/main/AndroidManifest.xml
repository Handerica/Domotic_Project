<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.domotic_project">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/housetic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AsyncQuery"></activity>
        <activity android:name=".Jardin" />
        <activity android:name=".Garaje" />
        <activity android:name=".Dormitorio" />
        <activity android:name=".Comedor" />
        <activity android:name=".Sala" />
        <activity android:name=".Estaciones" />
        <activity android:name=".LoginActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>